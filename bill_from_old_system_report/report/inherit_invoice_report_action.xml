<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
                <record id="neuland_it_solution_paperformate_id" model="report.paperformat">
            <field name="name">Neuland It Solution</field>
            <field name="default" eval="True"/>
            <field name="format">custom</field>
            <field name="page_height">297</field>
            <field name="page_width">210</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">40</field>
            <field name="margin_bottom">35</field>
            <field name="margin_left">20</field>
            <field name="margin_right">10</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">35</field>
            <field name="dpi">120</field>
        </record>


        <record id="industry_fsm_report.task_custom_report" model="ir.actions.report">
            <field name="paperformat_id" ref="neuland_it_solution_paperformate_id"/>
        </record>

    </data>
</odoo>